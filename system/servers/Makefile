.PHONY: default clean

default:
	@cd coco; $(MAKE)
	@cd init; $(MAKE)
	@cd session; $(MAKE)
	@cd winton; $(MAKE)
	@cd lamina; $(MAKE)
	@cd test; $(MAKE)

clean:
	cd coco; $(MAKE) clean
	cd init; $(MAKE) clean
	cd session; $(MAKE) clean
	cd winton; $(MAKE) clean
	cd lamina; $(MAKE) clean
	cd test; $(MAKE) clean
